#include <stdio.h>
#include <string.h>

int main(){

    int m1[3][2];
    int m2[2][4];
    int m3[3][4];

    printf("Put values of first array\n");
    for(int i=0; i<3; i++){
        for(int j=0; j<2; j++){
            scanf("%d",&m1[i][j]);
        }
    }

    printf("Put values of second array\n");
    for(int i=0; i<2; i++){
        for(int j=0; j<4; j++){
            scanf("%d",&m2[i][j]);
        }
    }

    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            m3[i][j]=0;
            for(int k=0; k<2; k++){
                m3[i][j]+=m1[i][k]*m2[k][j];
            }
        }
    }

    printf("Multiplication array\n");
    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            printf("%d ",m3[i][j]);
        }
        printf("\n");
    }

    return 0;
}

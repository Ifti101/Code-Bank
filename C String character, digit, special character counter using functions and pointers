#include <stdio.h>
#include <string.h>


void tc(char str[], int size);
void pc(char str[], int *a, int *d, int *s);
void pr(int a, int d, int s);

int main(){

    char str1[100];
    int a, d, s;

    tc(str1, 100);

    pc(str1, &a, &d, &s);

    pr(a, d, s);

    return 0;

}

void tc(char str[],int size){

    fgets(str, size, stdin);


}

void pc(char str[], int *a, int *d, int *s){

    *a=0;
    *d=0;
    *s=0;

    for(int i=0; str[i]!='\0'; i++){
        if((str[i]>='a' && str[i]<='z') || (str[i]>='A' && str[i]<='Z')){
            (*a)++;
        }
        else if(str[i]>='0' && str[i]<='9'){
            (*d)++;
        }
        else if(str[i]!=' '&& str[i]!='\n'){
            (*s)++;
        }
    }

}

void pr(int a, int d, int s){

    printf("Alphabet Count= %d\nDigit Count= %d\nSpecial Character Count= %d\n",a, d, s);

}
